&i2c_3 { /* BLSP1 QUP3 */
	status = "okay";
	himax-ts@48 {
		compatible = "himax,hxcommon";
		reg = <0x48>;
		interrupt-parent = <&tlmm>;
		interrupts = <65 0x2>;
		vdd-supply = <&pm8953_l17>;
		avdd-supply = <&pm8953_l6>;
		himax,panel-coords = <0 1080 0 2246>;
		himax,display-coords = <0 1080 0 2246>;
		himax,irq-gpio = <&tlmm 65 0x00>;
		himax,rst-gpio = <&tlmm 64 0x00>;
		himax,id0-gpio = <&tlmm 47 0x00>;
		himax,id1-gpio = <&tlmm 50 0x00>;
		report_type = <1>;
	};
};
/*add by wkh for tp begin*/
&i2c_3_active {
	config {
				drive-strength = <6>;
			};
};
&i2c_3_sleep {
	config {
				drive-strength = <6>;
			};
};
