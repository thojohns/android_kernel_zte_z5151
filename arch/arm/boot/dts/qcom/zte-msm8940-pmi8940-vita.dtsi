&tlmm {
		fp_shutdown_active:active{
				mux{
					pins = "gpio98";
					function = "gpio";
				};
				config{
					pins = "gpio98";
					drive-strength = <2>;
					output-high;
					bias-disable = <0>;
				};
		};
		fp_shutdown_suspend:suspend{
				mux{
					pins = "gpio98";
					function = "gpio";
				};
				config{
					pins = "gpio98";
					drive-strength = <2>;
					output-low;
					bias-disable = <0>;
				};
		};
		silead_irq_active:irq_active{
				mux{
					pins = "gpio97";
					function = "gpio";
				};
				config{
					pins = "gpio97";
					drive-strength = <2>;
					bias-pull-down;
				};
		};
		silead_pwr_active: silead_pwr_active {
				mux{
					pins = "gpio95";
					function = "gpio";
				};
				config {
					pins = "gpio95";
					drive-strength = <2>;
					bias-disable;
					output-high;
			};
		};
		goodix_irq_active:irq_active{
				mux{
					pins = "gpio97";
					function = "gpio";
				};
				config{
					pins = "gpio97";
					drive-strength = <2>;
					bias-pull-down;
				};
		};
		goodix_pwr_active: goodix_pwr_active {
				mux{
					pins = "gpio95";
					function = "gpio";
				};
				config {
					pins = "gpio95";
					drive-strength = <2>;
					bias-disable;
					output-high;
			};
		};

		pmx_fg_int_active {
			fg_int_active: fg_int_active {
				mux {
					pins = "gpio9";
